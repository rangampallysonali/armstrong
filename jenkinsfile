pipeline {
    agent {
        docker {
            image 'maven:3.9.6-eclipse-temurin-17'
        }
    }
        stages{
            stage('Chekout'){
                steps {
                      git branch:'main',url:'htpps://github.com/rangampallysonali/armstrong'
                }
            }
            stage('Build'){
                steps {
                    sh '''
                       mvn clean compile
                        '''
                            }
            }
            stage('test') {
                steps {
                    sh '''
                        mvn package
                       '''
            }
            }
            stage('Package') {
                steps {
                        mvn package
                }
            }
       }
       post {
        always {
            junit'**/target/surefire-reports/*.xml'
        }
   }
}
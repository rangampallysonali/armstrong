pipeline {
    agent {
        docker {
            image 'maven:3.9.6-eclipse-temurin-17'
        }
    }
        stages{
        stage('Compile'){
        steps {
            sh '''
            javac main.java
            '''
        }
        }
        stage('Run') {
            steps {
                sh '''
                java main.java
                '''
            }
        }
    }
}
